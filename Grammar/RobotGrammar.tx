Model:
    Products | Robot | Property
;
Robot:
    'robot' name=STRING '{'
        'properties' ('inherits from ' inherits*=[Robot|STRING][','])? '{'
            properties*=Property
        '}'
   '}'
;
Products:
    'products {'
        robots*=Robot
    '}'
;

Property:
    name=ID '=' value = PropertyValue
;

PropertyValue:
    STRING | BOOL | FLOAT | NUMBER
;

Comment:
  /\/\/.*$/
;